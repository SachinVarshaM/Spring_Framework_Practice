<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd"
	xmlns:p="http://www.springframework.org/schema/p">
	<bean class="com.sm.SpringCore.Emp" name="Emp1">

		<property name="name" value="Sachin" />

		<property name="phone">
			<list>
				<value>9874563258</value>
				<value>8574563218</value>
				<value>7788523642</value>
				<value>8946632175</value>
			</list>
		</property>

		<property name="address">
			<set>
				<value>Pune</value>
				<value>Mumbai</value>
				<value>Delhi</value>
			</set>
		</property>
		
		<property name="course">
			<map>

				<entry key="Java" value="4 Months" />

				<entry key="PHP" value="4 Months" />
				<entry key=".NET" value="4 Months" />
			</map>
		</property>
		
	</bean>

</beans>