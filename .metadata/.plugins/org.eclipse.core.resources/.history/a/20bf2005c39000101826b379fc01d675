import java.util.ArrayList;
import java.util.Arrays;
import java.util.Collection;
import java.util.Comparator;
import java.util.List;
import java.util.Optional;
import java.util.stream.Collector;

public class SecondHighestNumber {

	public static void main(String[] args) {
		List<Integer> list=Arrays.asList(1,32,33,44,53,99,43,67);
		Integer n=list.stream().distinct().sorted(Comparator.reverseOrder()).skip(1).findFirst().orElse(null);
        System.out.println(n);
        
        Optional<Integer> al=list.stream().distinct().filter(s->s!=0).findAny();
	}

}
