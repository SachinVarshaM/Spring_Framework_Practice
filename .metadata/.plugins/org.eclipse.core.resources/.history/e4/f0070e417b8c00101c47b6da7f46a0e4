
public class DuplicateString {

	public static void main(String[] args) {
		String str="AAABCBC";
		
		char[] str2=new char[10];
				str2=str.toCharArray();
		char ch;
		for(int i=0;i<str.length();i++)
		{
			ch=str.charAt(i);
			for(int j=i+1;j<str.length();j++)
			{
				if(ch==str.charAt(j))
				{
					str2[j]='';
				}
				else
				{
					
				}
				
			}
		}

	}

}
