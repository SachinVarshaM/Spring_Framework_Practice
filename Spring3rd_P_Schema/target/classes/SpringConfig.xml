<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
           
          <bean class="com.sm.Spring3rd_P_Schema.Employee" name="Emp1" p:empId="1001" p:empName="Sachin" p:empAddress="Velapur"/>
           
          <bean class="com.sm.Spring3rd_P_Schema.Employee" name="Emp2">
          
            <property name="empId" value="1002"/>
          <property name="empName" value="Varsha Mane"/>
          <property name="empAddress" value="Pune"/>       
          
          </bean>
</beans>